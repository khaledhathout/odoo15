<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record model="ir.module.category" id="purchase_return_category">
            <field name="name">Purchase Return</field>
        </record>


        <record id="group_purchase_return_user" model="res.groups">
            <field name="name">Purchase Return User</field>
            <field name="category_id" ref="wedo_purchase_return_managment.purchase_return_category"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
        </record>

   <record id="group_purchase_return_manager" model="res.groups">
            <field name="name">Purchase Return Manager</field>
            <field name="category_id" ref="wedo_purchase_return_managment.purchase_return_category"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="implied_ids" eval="[(4,ref('group_purchase_return_user'))]"/>
        </record>

    </data>
</odoo>