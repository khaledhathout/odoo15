<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="sale_return_report_paperformat" model="report.paperformat">
            <field name="name">Sale Return Report Paper Format</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">36</field>
            <field name="margin_bottom">7</field>
            <field name="header_spacing">38</field>
            <field name="dpi">90</field>
        </record>

        <report id="sale_return_report"
                model="sale.return.line"
                string="Sale Return Order Report"
                file="wedo_sale_return.sale_return_template"
                name="wedo_sale_return.sale_return_template"
                report_type="qweb-pdf"
                menu="False"
                paperformat="wedo_sale_return.sale_return_report_paperformat"
        />

        <report id="sale_return_report_xlsx"
                model="sale.return.line"
                string="Sale Return Order Report"
                file="wedo_sale_return.sale_rturn_report_wizard"
                name="wedo_sale_return.sale_return_template_xlsx"
                report_type="xlsx"
                menu="False"
        />

    </data>
</odoo>